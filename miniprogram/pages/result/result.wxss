@import "./../../style/icon.wxss";
page{
  background: #ededf1;
  padding-bottom:240rpx;
}
.banner_bk{
  width:750rpx;
  height:22%;
  position: absolute;
  left:0;
  top:0;
  z-index:-1;
}
.nav_top{
  width:100%;
  height:44rpx;
  display:flex;
  flex-direction:row;
  justify-content:flex-start;
  padding-top:60rpx;
  padding-bottom:30rpx;
}
.nav_top .nav_back{
  width:24rpx;
  height:44rpx;
  margin-left:24rpx;
}
.nav_top .nav_back image{
  display:block;
  width:24rpx;
  height:44rpx;
}
.nav_top  .nav_title{
  width:637rpx;
  height:44rpx;
  line-break:44rpx;
  font-size: 32rpx;
  font-weight:bold;
  text-align:center;
  color: #fff;
}
.my_tip{
  width:603rpx;
  height:74rpx;
  background: #c84342;
  border-radius:37rpx;
  text-align: center;
  margin:0 auto;
  line-height:74rpx;
}
.my_tip text{
  display: inline-block;
  font-size:20rpx;
  line-height:31rpx;
  color: #fff;
  vertical-align:middle;
}
.result_num{
  width:702rpx;
  margin:0 auto;
  display:flex;
  display:-webkit-flex;
  justify-content: space-between;
  height:73rpx;
  line-height:73rpx;
  padding:10rpx 0;
}
.result_nux{
  font-size:34rpx;
  font-weight:bold;
  color: #f5eeb8;
  padding-left:30rpx;
  display:inline-block;
}
.change_btn{
  font-size:34rpx;
  color: #fff;
  background:none;
  border:none;
  display: inline-block;
  padding:0;
  padding-right:30rpx;
  line-height:34rpx;
  height:34rpx;
  margin:0;
  margin-top:21rpx;
}
.change_btn::after{
  background:none;
  border:none;
}
.nav_wrap{
  width:700rpx;
  display:flex;
  display:-webkit-flex;
  margin:0 auto;
  flex-direction:row;
  justify-content:space-between;
  flex-wrap:wrap;
  padding-top:30rpx;
}
.nav_wrap .nav_item{
  width:130rpx;
  height:60rpx;
  border:1rpx solid #c2c2c2;
  line-height:60rpx;
  text-align: center;
  font-size:26rpx;
  color: #3b444e;
  background-color:#fff;
  margin-bottom:15rpx;
  border-radius:30rpx;
  transition: all 0.4s;
}
.nav_wrap .nav_item.nav_itemx{
  border:1px solid #ededf1;
  background-color: #ededf1;
}
.nav_wrap .nav_item.active{
  background-color: #d32423;
  border:1px solid #d32423;
  color: #fff;
}


.tip_cvover{
  width:100%;
  height:227rpx;
  position: fixed;
  bottom:0;
  left: 0;
  z-index: 999;
  background: rgba(0,0,0,0.6);
  border-radius:10rpx 10rpx 0 0;
  padding-top:1rpx;
  z-index:9999999999;
}
.tip_cvover.active{
  -webkit-animation: bounceInUp 1s;
  animation: bounceInUp 1s;
}
.tip_cvover .tip_con{
  width:700rpx;
  height:92rpx;
  background: #d32423;
  margin:0 auto;
  border-radius:15rpx;
  margin-top:66rpx;
  display:flex;
  display: -webkit-flex;
  justify-content: flex-start;
}
.tip_cvover .tip_con .close_tip{
  width:39rpx;
  height:39rpx;
  padding:26.5rpx;
  margin-left:24rpx;
}
.tip_cvover .tip_con .tip_text{
  width:469rpx;
  height:92rpx;
  font-size:32rpx;
  color: #fff;
  text-align: center;
  line-height:92rpx;
}
.tip_cvover .tip_con .tip_icon{
  width:22rpx;
  height:38rpx;
  padding:26.5rpx;
}

.nav_title{
  font-size: 28rpx;
  color: #3b444e;
  line-height:28rpx;
  width:700rpx;
  margin:0 auto;
  padding-top:50rpx;
  font-weight:bold;
}


.resultWrap{
  width:700rpx;
  margin:0 auto;
  border-top: 1rpx solid #c6c6c6;
  border-left: 1rpx solid #c6c6c6;
  border-right: 1rpx solid #c6c6c6;
}
.resultWrap .resultLine{
  width:100%;
  height:66rpx;
  border-bottom:1rpx solid #c6c6c6;
  overflow: hidden;
  display:flex;
  display:-webkit-flex;
  justify-content: space-between;
  flex-wrap:nowrap;
}
.resultWrap .resultLine.resultLine1{
  background: #d32423;
}
.resultWrap .resultLine.resultLine1 .resultLineItem{
  background:#d32423;
  color: #fff;
  border-right:1rpx solid #d32423;
  font-size:24rpx;
}
.resultWrap .resultLine.resultLine1 .resultLineItem5{
  border-right:none;
}
.resultWrap .resultLine .resultLineItem{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  text-align: center;
  background-color: #fff;
  border-right:1rpx solid #c6c6c6;
  line-height:66rpx;
  font-size:24rpx;
}
.resultWrap .resultLine .resultLineItem1{
  flex:2;
}
.resultWrap .resultLine .resultLineItem2{
  flex:4.5;
}
.resultWrap .resultLine .resultLineItem4{
  flex:1.5;
}
.resultWrap .resultLine .resultLineItem5{
  flex:2;
  border-right: none;
}
.resultWrap .resultLine .resultLineItem5 .btns{
  width:104rpx;
  height:25rpx;
}

.page_cover{
  position: fixed;
  left:0;
  top:0;
  bottom:0;
  right:0;
  background:rgba(0,0,0,0.5);
  z-index:99999;
}
.cover_con{
  width:680rpx;
  padding:20rpx;
  background: #c00101;
  border-radius:30rpx;
  position: fixed;
  left:50%;
  top:50%;
  margin-left:-360rpx;
  margin-top:-540rpx;
  z-index:9999999;
}
.knowBtns{
  width:400rpx;
  height:80rpx;
  background: #c00101;
  border-radius:20rpx;
  line-height:80rpx;
  text-align: center;
  position: absolute;
  bottom:-100rpx;
  left:50%;
  margin-left:-200rpx;
  color: #fff;
  font-size:36rpx;
}


.result_item{
  width:620rpx;
  margin:0 auto;
  border-radius:20rpx;
  background: #fff;
  border-radius:15rpx;
  -moz-box-shadow:0rpx 0rpx 7rpx #d6d7d3;
  -webkit-box-shadow:0rpx 0rpx 7rpx #d6d7d3;
  box-shadow:0rpx 0rpx 7rpx #d6d7d3;
  padding:30rpx;
  position: relative;
}

.result_code{
  display: block;
  font-size:30rpx;
  font-weight:bold;
  color: #3b444e;
  line-height:30rpx;
}
.result_name{
  display: block;
  font-size:34rpx;
  font-weight:bold;
  color: #3b444e;
  line-height:42rpx;
  margin:30rpx 0;
}
.result_detail{
  display:block;
  font-size:30rpx;
  color: #999999;
  line-height:49rpx;
  background: #f4f8f9;
  padding:20rpx;
  border-radius:10rpx;
}
.item_group{
  width:100%;
  padding-top:20rpx;
}
.item_group .item_line{
  display: flex;
  justify-content:flex-start;
  margin-bottom:14rpx;
  flex-direction: row;
  flex-wrap: wrap;
}
.item_group .item_line label{
  color: #969799;
  display: inline-block;
  margin-right:10rpx;
  font-size:28rpx;
  line-height:40rpx;
}
.item_group .item_line text{
  color: #46504f;
  display: inline-block;
  font-size:28rpx;
  line-height:40rpx;
}
.item_group .item_line.item_line_fs{
  height:50rpx;
  line-height:50rpx;
}
.item_group .item_line.item_line_fs label{
  line-height:50rpx;
}
.item_group .item_line.item_line_fs text{
  display: block;
  font-weight:bold;
  text-align: center;
  color: #d32423;
  font-size:40rpx;
  line-height:50rpx;
}

.show_more{
  width:26rpx;
  height:23rpx;
  display: block;
  margin:0 auto;
  padding:30rpx;
}
.add_btn{
  width:180rpx;
  height:50rpx;
  background: #d32423;
  line-height:50rpx;
  position:absolute;
  right:30rpx;
  top:20rpx;
  padding:0;
  text-align: center;
  color: #fff;
  border-radius:27rpx;
  font-size:22rpx;
}
.add_btn::after{
  border:none;
}
